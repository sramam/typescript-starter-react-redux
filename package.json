{
  "name": "typescript-starter-node",
  "version": "1.0.0",
  "description": "A starter package for typescript projects",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist/lib"
  ],
  "scripts": {
    "dev": "webpack-dev-server --env dev-server",
    "build": "run-batch",
    "build:prod": "run-batch",
    "build:dev-app": "run-batch",
    "build:dev-lib": "run-batch",
    "build:dist-app": "run-batch",
    "build:dist-lib": "run-batch",
    "static:dev": "static-server ./build/app",
    "static:prod": "static-server ./dist/app",
    "test:dev": "run-batch",
    "test:prod": "run-batch",
    "coverage": "istanbul check-coverage ./coverage/typescript/coverage.json",
    "sh": "npm-sh",
    "clean": "rimraf build coverage",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "build:watch": "tsc -w",
    "secure": "nsp check",
    "check": "npm-check -u",
    "_:check-engines:_": "check-engines",
    "_:css:_": "tcm ./src",
    "_:webpack:dev:_": "webpack --env development --colors --progress",
    "_:webpack:dev-app:_": "webpack --env dev-app --colors --progress",
    "_:webpack:dev-lib:_": "webpack --env dev-lib --colors --progress",
    "_:webpack:prod:_": "webpack --env production --colors --progress",
    "_:webpack:prod-app:_": "webpack --env prod-app --colors --progress",
    "_:webpack:prod-lib:_": "webpack --env prod-lib --colors --progress",
    "_:git-mods:allow-staged-mods:_": "git-mods --staged_ok",
    "_:git-mods:nothing-to-commit:_": "git-mods",
    "_:test:unit:clean:_": "rimraf ./build/unit-test",
    "_:test:unit:compile:_": "tsc -p ./config/tsconfig.unit-test.json",
    "_:test:unit:copy:_": "cpx \"src/**/*.{json,css}\" ./build/unit-test/src",
    "_:test:unit:run:_": "istanbul cover ./node_modules/.bin/_mocha",
    "_:remap-coverage:dev:_": "node ./scripts/remapped-coverage.js --force_min_cover false --output ./coverage/typescript",
    "_:remap-coverage:prod:_": "node ./scripts/remapped-coverage.js --force_min_cover true --output ./coverage/typescript",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "run-batch": {
    "build": [
      "_:check-engines:_",
      "clean",
      "lint",
      "_:css:_",
      "_:webpack:dev:_"
    ],
    "build:prod": [
      "_:check-engines:_",
      "clean",
      "lint",
      "_:css:_",
      "_:webpack:prod:_"
    ],
    "build:dev-app": [
      "_:check-engines:_",
      "clean",
      "lint",
      "_:css:_",
      "_:webpack:dev-app:_"
    ],
    "build:dev-lib": [
      "_:check-engines:_",
      "clean",
      "lint",
      "_:css:_",
      "_:webpack:dev-lib:_"
    ],
    "build:prod-app": [
      "_:check-engines:_",
      "clean:dist",
      "lint",
      "_:css:_",
      "_:webpack:prod-app:_"
    ],
    "build:prod-lib": [
      "_:check-engines:_",
      "clean:dist",
      "lint",
      "_:css:_",
      "_:webpack:prod-lib:_"
    ],
    "test:prod": [
      "_:test:unit:clean:_",
      "_:test:unit:compile:_",
      "_:test:unit:copy:_",
      "_:test:unit:run:_",
      "_:remap-coverage:prod:_"
    ],
    "test:dev": [
      "_:test:unit:clean:_",
      "_:test:unit:compile:_",
      "_:test:unit:copy:_",
      "_:test:unit:run:_",
      "_:remap-coverage:dev:_"
    ]
  },
  "pre-commit": [
    "test:dev",
    "build:prod",
    "_:git-mods:allow-staged-mods:_"
  ],
  "pre-push": [
    "test:prod",
    "build:prod",
    "check",
    "secure",
    "_:git-mods:nothing-to-commit:_"
  ],
  "keywords": [
    "TypeScript",
    "starter",
    "node"
  ],
  "author": "Shishir Ramam",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^3.4.30",
    "@types/enzyme": "^2.5.39",
    "@types/expect": "^1.13.31",
    "@types/mocha": "^2.2.35",
    "@types/node": "^6.0.55",
    "@types/react": "^0.14.52",
    "@types/react-addons-test-utils": "^0.14.16",
    "@types/react-dom": "^0.14.20",
    "@types/react-redux": "^4.4.35",
    "@types/react-router": "^2.0.41",
    "@types/react-router-redux": "^4.0.36",
    "@types/redux-devtools": "^3.0.36",
    "@types/redux-devtools-dock-monitor": "^1.1.29",
    "@types/redux-devtools-log-monitor": "^1.0.29",
    "@types/reselect": "^2.0.27",
    "@types/sinon": "^1.16.34",
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "check-engines": "^1.2.0",
    "clean-webpack-plugin": "^0.1.14",
    "commander": "^2.9.0",
    "cpx": "^1.5.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "env-cmd": "^4.0.0",
    "enzyme": "^2.7.0",
    "getport": "^0.1.0",
    "git-mods": "^1.0.1",
    "graceful-fs": "^4.1.5",
    "html-webpack-plugin": "^2.25.0",
    "imports-loader": "^0.7.0",
    "istanbul": "^0.4.4",
    "jscrambler-loader": "^2.0.2",
    "licensecheck": "^1.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.1",
    "node-emoji": "^1.4.3",
    "npm-check": "^5.2.3",
    "npm-run-batch": "0.0.4",
    "npm-sh": "^1.0.1",
    "nsp": "^2.6.1",
    "nsp-formatter-checkstyle": "^1.0.1",
    "nsp-formatter-remarkup": "^1.0.2",
    "postcss-reporter": "^3.0.0",
    "pre-commit": "^1.1.3",
    "pre-push": "^0.1.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.3.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-chart-monitor": "^1.4.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-extension": "^1.0.0",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-slider-monitor": "^1.0.7",
    "remap-istanbul": "^0.8.4",
    "rimraf": "^2.5.4",
    "semver": "^5.3.0",
    "shelljs-nodecli": "^0.1.1",
    "sinon": "^1.17.5",
    "source-map-loader": "^0.1.5",
    "static-server": "^2.0.3",
    "stats-webpack-plugin": "^0.4.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.3.3",
    "ts-node": "^2.0.0",
    "tslint": "^4.2.0",
    "tslint-loader": "^3.3.0",
    "typed-css-modules": "^0.1.12",
    "typescript": "^2.1.4",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony",
    "webpack": "^2.2.0-rc.2",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-closure-compiler": "^2.1.2",
    "webpack-dev-server": "^2.2.0-rc.0",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-merge": "^2.0.0",
    "webpack-validator": "^2.3.0",
    "webpack-vendor-chunk-plugin": "^1.0.0"
  },
  "dependencies": {
    "check-engines": "^1.4.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-focus": "^1.0.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-saga": "^0.14.0",
    "reselect": "^2.5.3",
    "window-or-global": "^1.0.1",
    "snyk": "^1.316.1"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "snyk": true
}
